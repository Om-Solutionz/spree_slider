<% if current_store.present? && current_store.code != "spree" %>
  <%= render partial: "spree/#{current_store.code}/shared/slider", locals: { slider: slider, cid: cid, interval: interval } %>
<% else %>
  <%= render partial: "spree/shared/default_slider", locals: { slider: slider, cid: cid, interval: interval } %>
<% end %>